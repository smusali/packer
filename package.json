{
  "name": "packaging",
  "version": "1.0.0",
  "description": "Solution for Packaging Problem Asked by Mobiquity Europe",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint **/*.ts",
    "pretest": "npm run lint",
    "start": "nodemon index.ts",
    "test": "tap --node-arg=--require=./node_modules/ts-node/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smusali/packaging.git"
  },
  "keywords": [],
  "author": "Samir Musali <samir.musali@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smusali/packaging/issues"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "ignorePatterns": [
      "node_modules/",
      "coverage/"
    ]
  },
  "tap": {
    "ts": true,
    "reporter": "tap",
    "output-file": ".tap-output",
    "check-coverage": true,
    "files": [
      "test/**/*.ts"
    ],
    "coverage-report": [
      "text",
      "text-summary",
      "json",
      "json-summary",
      "html"
    ],
    "branches": 92,
    "lines": 97,
    "functions": 100,
    "statements": 96,
    "nyc-arg": [
      "--exclude=test/",
      "--exclude=coverage/",
      "--all"
    ]
  },
  "homepage": "https://github.com/smusali/packaging#readme",
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/express": "^4.17.13",
    "@types/node": "^16.9.2",
    "@types/tap": "^15.0.5",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "nodemon": "^2.0.13",
    "tap": "^15.0.10",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "uuid": "^8.3.2"
  }
}
